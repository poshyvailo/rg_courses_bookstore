%table.table.table-hover
  %thead
    %tr
      %th
        %span.in-grey-600 Number
      %th
        %span.in-grey-600 Completed at
      %th
        %span.in-grey-600 Status
      %th
        %span.in-grey-600 Total
  %tbody
    - @orders.each do |order|
      %tr
        %td
          %a.general-order-number{href: customers_order_path(order)}= order.order_number
        %td
          %span.in-grey-900.font-16.fw-300= order.completed_at
        %td
          %span.font-16.in-grey-900.fw-300= order.state
        %td
          %strong.font-16= currency(order.total_price)