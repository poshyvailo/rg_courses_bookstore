%h1.general-title-margin= t('settings.title')
%ul.nav.clearfix.mb-20{ role: 'tablist' }
  %li.tab-item.active{ role: 'presentation' }
    %a.tab-link{ href: '#address',
      'aria-controls': 'address',role: 'tab', 'data-toggle': 'tab' }= t('settings.tab_address')
  %li.tab-item{ role: 'presentation' }
    %a.tab-link{ href: '#privacy', 'aria-controls': 'privacy', role: 'tab', 'data-toggle': 'tab' }= t('settings.tab_privacy')
.tab-content
  #address.tab-pane.fade.in.active.general-main-wrap{ role: 'tabpanel' }
    = render 'settings_address'
  #privacy.tab-pane.fade{ role: 'tabpanel' }
    = render 'setting_privacy'
